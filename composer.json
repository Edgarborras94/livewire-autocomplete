{
    "name": "joshhanley/livewire-autocomplete",
    "description": "An autocomplete dropdown for Livewire",
    "license": "MIT",
    "authors": [
        {
            "name": "Josh Hanley",
            "email": "josh@joshhanley.com.au"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0|dev-master",
        "livewire/livewire": "^3|dev-master"
    },
    "autoload": {
        "psr-4": {
            "LivewireAutocomplete\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LivewireAutocomplete\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LivewireAutocomplete\\LivewireAutocompleteServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "joshhanley/livewire-dusk-testbench": "^0.1.0",
        "spatie/laravel-ray": "^1.19"
    }
}
